{
    "name": "drupal/legacy-project",
    "description": "Project template for Drupal 8 projects with composer following drupal/drupal layout",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "cocur/slugify": "^4.3",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.7",
        "drupal/admin_toolbar": "^3.0",
        "drupal/adminimal_admin_toolbar": "^1.11",
        "drupal/adminimal_theme": "^1.6",
        "drupal/azure_appinsights": "^2.0@alpha",
        "drupal/backup_migrate": "^5.0",
        "drupal/block_content_suggestions": "^1.0",
        "drupal/bulkdelete": "1.x-dev@dev",
        "drupal/calendar": "^1.0@alpha",
        "drupal/color_field": "3.0",
        "drupal/conditional_fields": "^4.0@alpha",
        "drupal/core-composer-scaffold": "^9",
        "drupal/core-project-message": "^9",
        "drupal/core-recommended": "^9.3.3",
        "drupal/core-vendor-hardening": "^8.8",
        "drupal/cshs": "^3.5",
        "drupal/csv_serialization": "^2.0",
        "drupal/ctools": "^3.7",
        "drupal/devel": "^4.1",
        "drupal/draggableviews": "^1.3",
        "drupal/dynamic_breadcrumb": "^1.4",
        "drupal/editor_advanced_link": "^1.8",
        "drupal/entity_type_clone": "^3.0",
        "drupal/filefield_paths": "^1.0@beta",
        "drupal/fullcalendar_view": "^5.1",
        "drupal/libraries": "^4.0",
        "drupal/maxlength": "^2.0@RC",
        "drupal/menu_item_extras": "^3.0",
        "drupal/metatag": "^1.19",
        "drupal/paragraphs": "^1.15",
        "drupal/pathauto": "^1.8",
        "drupal/scheduler": "2.0",
        "drupal/scss_compiler": "^1.1",
        "drupal/search_api": "^1.23",
        "drupal/shs": "^2.0@RC",
        "drupal/stage_file_proxy": "^2.0",
        "drupal/structure_sync": "2.0",
        "drupal/toolbar_menu": "^2.2",
        "drupal/transliterate_filenames": "^1.5",
        "drupal/twig_tweak": "^3.2",
        "drupal/views_base_url": "^2.0@beta",
        "drupal/views_contextual_filters_or": "1.4",
        "drupal/views_data_export": "^1.0",
        "drupal/webform": "^6.1",
        "drush/drush": "^11.6",
        "phpmailer/phpmailer": "^6.2",
        "symfony/console": "*",
        "symfony/deprecation-contracts": "*",
        "symfony/error-handler": "*",
        "symfony/event-dispatcher": "*",
        "symfony/event-dispatcher-contracts": "*",
        "symfony/finder": "*",
        "symfony/http-foundation": "*",
        "symfony/http-kernel": "*",
        "symfony/mailer": "*",
        "symfony/mime": "*",
        "symfony/process": "*",
        "symfony/routing": "*",
        "symfony/service-contracts": "*",
        "symfony/string": "*",
        "symfony/translation": "*",
        "symfony/translation-contracts": "*",
        "symfony/var-dumper": "*"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "bin-dir": "bin/",
        "secure-http": false,
        "preferred-install": {
          "drupal/core": "dist"
        },        
		"optimize-autoloader": true,
		"prepend-autoloader": false,
		"platform": {
			"php": "8.1"
		},          
        "platform-check": false,
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "drupal/core-vendor-hardening": true,
            "cweagans/composer-patches": true
        }
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "./"
            }
        },
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "libraries/{$name}": [
                "type:drupal-library"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/legacy-project template!               </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        },
        "enable-patching": "true",
        "patches": {
            "drupal/conditional_fields_tab_route_does_not_exist_anymore": {
                "Working for 8.4?": "https://www.drupal.org/files/issues/2020-06-30/conditional_fields-conditional_fields_tab_route_does_not_exist_anymore-3155890-d8-2.patch"
            },            
            "drupal/conditional_fields": {
                "Working for 8.4?": "https://www.drupal.org/files/issues/2021-11-10/3203212-6.patch"
            }
        }        
    }
}
